{
  "version": 1,
  "projects": {
    "mjwebsite": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/mjwebsite",
      "sourceRoot": "apps/mjwebsite/src",
      "prefix": "front",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/mjwebsite",
            "index": "apps/mjwebsite/src/index.html",
            "main": "apps/mjwebsite/src/main.ts",
            "polyfills": "apps/mjwebsite/src/polyfills.ts",
            "tsConfig": "apps/mjwebsite/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/mjwebsite/src/favicon.ico",
              "apps/mjwebsite/src/assets"
            ],
            "styles": [
              "apps/mjwebsite/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mjwebsite/src/environments/environment.ts",
                  "with": "apps/mjwebsite/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mjwebsite:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mjwebsite:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mjwebsite:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/mjwebsite/tsconfig.app.json",
              "apps/mjwebsite/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/mjwebsite/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mjwebsite/jest.config.js",
            "tsConfig": "apps/mjwebsite/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/mjwebsite/src/test-setup.ts"
          }
        }
      }
    },
    "mjwebsite-e2e": {
      "root": "apps/mjwebsite-e2e",
      "sourceRoot": "apps/mjwebsite-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mjwebsite-e2e/cypress.json",
            "tsConfig": "apps/mjwebsite-e2e/tsconfig.e2e.json",
            "devServerTarget": "mjwebsite:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mjwebsite:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/mjwebsite-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/mjwebsite-e2e/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "9e5bc92b-04aa-485e-bc64-42d7ccde3603"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "mjwebsite"
}
